{
    "use_proxy": true,
    "retry_settings": {
        "max_retries": 3,
        "retry_delay": 5,
        "rate_limit_reset_time": 60
    },
    "timing": {
        "min_interval": 180,
        "max_interval": 300,
        "account_delay": {
            "min": 1.0,
            "max": 3.0
        }
    },
    "auto_restart": {
        "enabled": true,
        "hours": 1,
        "clear_error_caches": true,
        "min_success_rate": 0.3,
        "min_cycles": 2
    },
    "ssl_settings": {
        "verify": false,
        "disable_warnings": true
    },
    "output": {
        "show_proxy_info": true,
        "show_detailed_errors": true,
        "color_output": true
    },
    "logging": {
        "enabled": true,
        "level": "INFO",
        "rotate_size": 10485760,
        "backup_count": 5
    }
}
